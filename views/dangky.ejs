<form action="/thanhvien/luu" method="POST" class="page-section" id="contact">
  <div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
      <div class="col-lg-8 col-xl-6 text-center">
        <h2 class="mt-0">Đăng kí thành viên</h2>
      </div>
    </div>
    <div class="row gx-4 gx-lg-5 justify-content-center mb-5">
      <div class="col-lg-6">
        <form id="contactForm">
          <!-- LastName input-->
          <div class="form-floating mb-3">
            <input class="form-control" name="ho" id="ho" type="text" placeholder="Vui lòng điền họ"
              data-sb-validations="required" />
            <label for="ho">Họ</label>
          </div>
          <!-- FirstName input-->
          <div class="form-floating mb-3">
            <input class="form-control" name="ten" id="ten" type="text" placeholder="vui lòng điền tên"
              data-sb-validations="required" />
            <label for="ten">Tên</label>
          </div>
          <!--Username input-->
          <div class="form-floating mb-3">
            <input class="form-control" name="username" id="un" type="text" placeholder="(123) 456-7890"
              data-sb-validations="required" />
            <label for="un">Username</label>
          </div>
          <!-- Passwword input -->
          <div class="form-floating mb-3">
            <input class="form-control" name="password" id="pass" type="password" placeholder="(tuyy)"
              data-sb-validations="required" />
            <label for="pass">Mật khẩu</label>
          </div>
          <!-- Email address input-->
          <div class="form-floating mb-3">
            <input class="form-control" name="email" id="email" type="email" placeholder="name@example.com"
              data-sb-validations="required,email" />
            <label for="email">Email address</label>
          </div>
          <!-- Favourite input-->
          <div class="form-floating mb-3">
            <textarea class="form-control" name="sothich" id="fv" type="text" placeholder="Vui lòng điền sở thích"
              style="height: 8rem" data-sb-validations="required"></textarea>
            <label for="favourite">Sở Thích</label>
          </div>
          <!-- Address Input -->
          <div class="form-floating mb-3">
            <input class="form-control" name="tinh" id="tinh" type="text" placeholder="Vui lòng điền tỉnh"
              data-sb-validations="required" />
            <label for="tinhthanh">Tỉnh Thành</label>
          </div>
          <div class="d-grid"><button class="btn btn-primary px-4  py-2 text-white" type="submit">ĐĂNG KÝ</button></div>
        </form>
      </div>
    </div>

  </div>
</form>
